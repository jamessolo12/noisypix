* { padding: 0; margin: 0; }

body {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 13px;
	background: url("http://noisyimages.s3.amazonaws.com/25original.gif") repeat scroll 0 0 transparent;
}

h1 {
    background-image: url("http://noisyimages.s3.amazonaws.com/cloudPix2.png");
    position:relative;
    top:20px;
    left:30px;
    padding-top: 88px;
    height: 0px;
    overflow: hidden;
    background-repeat: no-repeat;
}

#wrapper { 
    margin-left: auto;
    margin-right: auto;
    width: 70%;
    background: #FFFFFF;
    top:0; 
    left:0; 
}

#content { 
    background: #FFFFFF;
    min-height: 250px;
    padding-left: 50px;
    padding-right: 50px;
    padding-bottom: 16px;
    padding-top: 0px;
}

#header {
	height: 120px;
	background: #FFFFCC;
}

.formContent {
    margin-top:20px;
    margin-left:150px;
}

.formContentSmaller {
    margin-top:10px;
    margin-left:150px;
}

fieldset {
    border: 0;
}

#socialMedia {
	padding: 5px;
	margin-top: 5px;
}

.contentBox {
	border-style: solid;
	border-width: 1px;
	padding: 5px;
	margin: 20px;
	min-height: 95px;
}

.boxedImg {
	float: left;
	position: relative;
}

#step4_terms p {
	font-size: 11px;
}

#footer {
    background: #424242;
    padding: 10px;
}

#footer A:link {
	text-decoration: none;
	color: black;
}
#footer A:visited {
	text-decoration: none;
	color: black;
}
#footer A:active {
	text-decoration: none;
	color: black;
}
#footer A:hover {
	text-decoration: underline; 
	color: black;
}
